<!DOCTYPE html>
<html lang="en">

<head>
  <title>Home Page</title>

<!-- Test -->

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

  <!--Import Google Icon Font-->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!-- Compiled and minified CSS -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
  <!--Let browser know website is optimized for mobile-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

</head>

<body class="grey lighten-5">

  <!-- Navbar -->
  <nav>
    <div class="nav-wrapper grey lighten-5 ">
      <a href="#!" class="brand-logo left pink-text text-darken-4">whattado</a>
      <ul class="login row right">
        <li class="right login-clicker">
          <a class="modal-trigger pink-text text-darken-3" href="#login-modal">login</a>
        </li>
      </ul>
    </div>
  </nav>

  <!-- Login Modal -->
  <div id="login-modal" class="modal">
    <div class="modal-content">
      <section class="section row">
        <div class="input-field col s8">
          <input value="1" id="login-id" type="text" class="validate">
          <label class="active pink-text text-darken-2" for="login-id">login id</label>
        </div>
        <button class="waves-effect waves-light btn-large pink darken-3 col s3 offset-s1">login</button>
      </section>
    </div>
  </div>

  <!-- Main -->
  <main>
    <!--Weird button slide toggle without this-->
    <section class="section"></section>
    <!-- Whattado Button Form -->
    <section class="">
      <form class="whattado container">

        <!-- Time Radios -->
        <div class="row section times">
          <button class="col s3 waves-effect waves-light btn-large pink darken-1 flow-text 1h" value="true">
            1Hour
          </button>
          <button
            class="col s3 waves-effect waves-light btn-large pink-text text-darken-1 grey lighten-4 z-depth-0 flow-text 3h"
            value="false">
            3Hours
          </button>
          <button
            class="col s3 waves-effect waves-light btn-large pink-text text-darken-1 grey lighten-4 z-depth-0 flow-text 6h"
            value="false">
            6Hours
          </button>
          <button
            class="col s3 waves-effect waves-light btn-large pink-text text-darken-1 grey lighten-4 z-depth-0 flow-text 24h"
            value="false">
            1Day
          </button>
        </div>

        <!-- Group Radios -->
        <div class="row section groups">
          <button
            class="col s3 waves-effect waves-light btn-large pink-text text-darken-2 grey lighten-4 z-depth-0 solo"
            value="false">
            <i class="material-icons">person</i>
          </button>
          <button class="col s3 waves-effect waves-light btn-large pink-text text-darken-2 grey lighten-4 z-depth-0 duo"
            value="false">
            <i class="material-icons">group</i>
          </button>
          <button
            class="col s3 waves-effect waves-light btn-large pink-text text-darken-2 grey lighten-4 z-depth-0 group"
            value="false">
            <i class="material-icons">group_add</i>
          </button>
          <button class="col s3 waves-effect waves-light btn-large pink darken-2 family" value="true">
            <i class="material-icons">child_friendly</i>
          </button>
        </div>

        <!-- Type Toggles -->
        <div class="row types">
          <div class="col s3">
            <p class="center-align">
              <button class="btn-floating btn-large waves-effect waves-light red accent-4 movies" value="true"><i
                  class="material-icons">movie_filter</i></button>
            </p>
          </div>
          <div class="col s3">
            <p class="center-align">
              <button class="btn-floating btn-large waves-effect waves-light indigo darken-2 books" value="true"><i
                  class="material-icons">menu_book</i></button>
            </p>
          </div>
          <div class="col s3">
            <p class="center-align">
              <button class="btn-floating btn-large waves-effect waves-light grey lighten-4 music" value="false"><i
                  class="teal-text text-darken-3 material-icons">music_note</i></button>
            </p>
          </div>
          <div class="col s3">
            <p class="center-align">
              <button class="btn-floating btn-large waves-effect waves-light amber darken-4 restaurants" value="true"><i
                  class="material-icons">restaurant</i></button>
            </p>
          </div>
        </div>
        <div class="row submit">
          <button class="col s6 offset-s3 waves-effect waves-light btn-large center-align pink darken-3 hoverable"
            type="submit">
            <img src="https://img.icons8.com/ios/50/000000/paper-waste.png" style="filter: invert(100%)">
          </button>
        </div>

      </form>
    </section>

          <!-- Spinner -->
          <div class="row" id="todo-spinner">
              <div class="col offset-s5">
                <div class="preloader-wrapper small active">
                  <div class="spinner-layer spinner-blue-only">
                    <div class="circle-clipper left">
                      <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                      <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                      <div class="circle"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
    <section class="todos container col s12">

    </section>
    <!--Search button-->
    <section>

      <div class="fixed-action-btn click-to-toggle vertical">
        <a class="btn-floating btn-large click-to-toggle green darken-3" id="new-search">
          <i class="large material-icons">add</i>
        </a>
        <form class="search-form" method="GET" action="/allTodos" style="padding: 3px;
          float: left;">
          <input type="search" placeholder="Search...">
        </form>
      </div>
      </div>
    </section>
    <footer>
      <div class="paging-container" id="demo"> </div>
    </footer>

  </main>

  <!--JavaScript at end of body for optimized loading-->
  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script type="text/javascript" src="/scripts/home-button.js"></script>
  <script type="text/javascript" src="/scripts/login.js"></script>
  <script type="text/javascript" src="/scripts/whattado-form.js"></script>
  <script type="text/javascript" src="/scripts/user-todo.js"></script>
  <script type="text/javascript" src="/scripts/search.js"></script>

  <!-- Test -->



</body>

</html>
